<class 'OWScript.AST.Numeral'>
<class 'OWScript.AST.BinaryOp'>
rule("Generated by https://github.com/adapap/OWScript") { Event { Ongoing - Global; }}
rule("Advanced Arrays") {
   Event {
      Ongoing - Global;
   }
   Actions {
      Set Global Variable At Index(A, 0, Empty Array);
      Set Global Variable At Index(A, 1, Append To Array(Append To Array(Append To Array(Empty Array, 1), 2), 3));
      Set Global Variable(B, Value In Array(Global Variable(A), 0));
      Set Global Variable At Index(B, 0, 2);
      Set Global Variable At Index(A, 0, Global Variable(B));
      Set Global Variable(B, Value In Array(Global Variable(A), 0));
      Set Global Variable At Index(B, 1, Add(Value In Array(Value In Array(Global Variable(A), 1), 1), Value In Array(Value In Array(Global Variable(A), 1), 2)));
      Set Global Variable At Index(A, 0, Global Variable(B));
      Modify Global Variable At Index(A, 0, Append To Array, Vector(5, 5, 5));
      Modify Global Variable At Index(A, 0, Append To Array, Vector(6, 6, 6));
   }
}

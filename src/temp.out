rule("String Builder") {
   Event {
      Ongoing - Each Player;
      All;
      All;
   }

   Actions {
      Set Player Variable At Index(Event Player, A, 0, 100);
      Small Message(All Players(Team(All)), String("{0}: {1}", String("Money", Null, Null, Null), String("{0}!", Value In Array(Player Variable(Event Player, A), 0), Null, Null), Null));
   }
}
rule("If and Else") {
   Event {
      Ongoing - Global;
   }

   Actions {
      Set Global Variable At Index(A, 0, 1);
      Skip If(Not(Compare(X, >, 1)), 3);
      Set Global Variable At Index(A, 0, 2);
      Set Global Variable At Index(A, 1, 1);
      Skip(1)
      Set Global Variable At Index(A, 0, 5);
   }
}